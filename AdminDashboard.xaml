<Window x:Class="TicketTest.AdminDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicketTest"
        mc:Ignorable="d"
        Title="AdminDashboard" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="TicketList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,63,0,0"/>
        <Label x:Name="lblTicketList" Content="List of Tickets" HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top" FontSize="20"/>
    </Grid>
</Window>
